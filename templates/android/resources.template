var props = _.filter(allProperties, this.filter||{});
function isSize(prop)     { return prop.attributes.category === 'size'; };
function isColor(prop)    { return prop.attributes.category === 'color'; };
function isNumber(prop)   { return prop.attributes.category === 'number'; };
%>
<!--
