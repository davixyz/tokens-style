-->
<resources>
  <% _.each(props, function(prop) {
    if (isSize(prop)) { %><dimen name="<%= prop.name %>"><%= prop.value %></dimen><% if (prop.comment) { %><!-- <%= prop.comment %> --><% }
    } else if (isColor(prop)) { %><color name="<%= prop.name %>"><%= prop.value %></color><% if (prop.comment) { %><!-- <%= prop.comment %> --><% }
    } else if (isNumber(prop)) { %><item name="<%= prop.name %>" type="dimen" format="float"><%= prop.value %></item><% if (prop.comment) { %><!-- <%= prop.comment %> --><% }
    } %>
  <% }); %>
</resources>
